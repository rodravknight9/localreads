@page "/profile"
@using Microsoft.AspNetCore.Authorization
@attribute [Authorize]

@inject AppState State

<MudElement>
    <MudText>@User.UserName</MudText>
</MudElement>

@code {

    private AuthResponse User = new AuthResponse();
    protected override void OnInitialized()
    {
        User = State.UserState.User;
    }

}